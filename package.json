{
  "name": "deerflow-node",
  "version": "0.1.0",
  "description": "Node.js implementation of DeerFlow - Deep Exploration and Efficient Research Flow",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "dev:server": "pnpm --filter server dev",
    "dev:web": "pnpm --filter web dev",
    "start": "pnpm -r start",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "install:all": "pnpm install"
  },
  "keywords": [
    "ai",
    "research",
    "workflow",
    "llm",
    "agents",
    "monorepo"
  ],
  "author": "Your Name",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@eslint/js": "^9.27.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.11.0"
}